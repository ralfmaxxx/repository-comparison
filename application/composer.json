{
    "name": "ralfmaxxx/repository-comparison",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "7.3.6",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "knplabs/github-api": "^2.11",
        "league/tactician-bundle": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "spatie/url": "^1.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "4.3.*",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/mink": "dev-master",
        "behat/symfony2-extension": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "mockery/mockery": "^1.2",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^5.1",
        "phpstan/phpstan": "^0.12.11",
        "phpunit/phpunit": "^8.2",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Engine\\": "symfony/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "database-preparation": {
            "database:create": "bin/console doctrine:database:create --if-not-exists",
            "database:migrate": "bin/console doctrine:migrations:migrate --no-interaction --allow-no-migration"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@database-preparation"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
